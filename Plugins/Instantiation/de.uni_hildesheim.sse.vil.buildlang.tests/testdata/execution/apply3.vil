vilScript apply3 (Project source, Configuration config, Project target) {

    version v0;

    typedef FunCall callOf(Integer);

    func(Integer i) = : {
        println(i);
    }

    main() = : {
        sequenceOf(Integer) seq = {1, 2, 3, 4, 5, 6};
        iter(seq, func);
    }

    iter(sequenceOf(Integer) seq, FunCall r) = : {
        map(i:seq) {
            r(i);
        };
    }

}