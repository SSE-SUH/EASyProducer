project PipelinesCfgTSI {
 	
	import Pipelines;
	import FamiliesCfg;
	import DataManagementCfg;
	
	Source src_TSI;
    Sink snk_TSI;
	FamilyElement peNormalize;          
	FamilyElement peAdd10;
	FamilyElement peDouble;
	FamilyElement peShow;
   
	Flow f1_TSI;
	f1_TSI = {
		name = "src-normalize",
		destination = refBy(peNormalize),
		grouping = Grouping.shuffleGrouping		
	}; 
		          
	Flow f2_TSI;     
	f2_TSI = {
		name = "normalize-fAdd",
		destination = refBy(peAdd10),
		grouping = Grouping.shuffleGrouping		
	};
     
	Flow f3_TSI;
	f3_TSI = { 
		name = "fAdd-double",
		destination = refBy(peDouble),
		grouping = Grouping.shuffleGrouping
	};
	
	Flow f4_TSI;
	f4_TSI = {
		name = "double-show",
		destination = refBy(peShow),
		grouping = Grouping.shuffleGrouping
	};
	
	
	Flow f5_TSI;
	f5_TSI = {
		name = "show-snk",
		destination = refBy(snk_TSI),
		grouping = Grouping.shuffleGrouping
	};

    src_TSI.name = "src";
	src_TSI.output = {refBy(f1_TSI)};
    src_TSI.source = dataSources[0];
    
	peNormalize.name="peNormalize";
	peNormalize.family = refBy(fNormalize);  
	peNormalize.output = {refBy(f2_TSI)}; 

	peAdd10.name="peAdd10";
	peAdd10.family = refBy(fAdd10);
	peAdd10.output = {refBy(f3_TSI)}; 

    peDouble.name="peDouble";
    peDouble.family = refBy(fDouble);
    peDouble.output = {refBy(f4_TSI)};
    
    peShow.name="peShow";
    peShow.family = refBy(fShow);
    peShow.output = {refBy(f5_TSI)};

    snk_TSI.name = "snk";
	snk_TSI.sink = dataSinks[0];

	Pipeline pipTSI;
	pipTSI = Pipeline {
		name = "pipTSI",
		sources = {refBy(src_TSI)},
		numworkers = 1
	};

   
	
	freeze {
        f1_TSI;
        f2_TSI;
        f3_TSI;
        f4_TSI;
        f5_TSI;
        src_TSI;
        snk_TSI;
        peNormalize;
        peAdd10;
        peDouble;
        peShow;
        pipTSI;
		
    }     

}