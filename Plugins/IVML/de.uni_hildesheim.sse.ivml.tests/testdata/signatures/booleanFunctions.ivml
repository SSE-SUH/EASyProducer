project booleanFunctions {

    Boolean var = false;
    String s;
    var == false;
    var <> true;
    //* var <> true;
    <>(var, true);
    //* var <> true;
    var .<>(true);
    //* var <> true;
    var != true;
    //* var != true;
    !=(var, true);
    //* var != true;
    var .!=(true);
    isDefined(var);
    //* isDefined(var);
    var.isDefined();
    isKindOf(var, Boolean);
    //* isKindOf(var, Boolean);
    var.isKindOf(Boolean);
    isTypeOf(var, Boolean);
    //* isTypeOf(var, Boolean);
    var.isTypeOf(Boolean);
    s = locale(var);
    //* s = locale(var);
    s = var.locale();
    isKindOf(var, typeOf(var));
    //* isKindOf(var, typeOf(var));
    var.isKindOf(var.typeOf());
    s = asType(var, String);
    //* s = asType(var, String);
    s = var.asType(String);
    var = var and true;
    //* var = var and true;
    var = and(var, true);
    //* var = var and true;
    var = var.and(true);
    var implies true;
    //* var implies true;
    implies(var, true);
    //* var implies true;
    var.implies(true);
    var = var iff true;
    //* var = var iff true;
    var = iff(var, true);
    //* var = var iff true;
    var = var.iff(true);
    var = var or true;
    //* var = var or true;
    var = or(var, true);
    //* var = var or true;
    var = var.or(true);
    var = var xor true;
    //* var = var xor true;
    var = xor(var, true);
    //* var = var xor true;
    var = var.xor(true);
    var = not var;
    //* var = not var;
    var = var.not();
    s = toString(var);
    //* s = toString(var);
    s = var.toString();

}