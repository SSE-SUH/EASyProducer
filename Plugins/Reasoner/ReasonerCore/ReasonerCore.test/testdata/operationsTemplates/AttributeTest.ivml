project AttributeTest {

    version v0;    
    
    enum BindingTime {compile, startup, runtime};
    
    attribute BindingTime bindingTime = BindingTime.compile to AttributeTest;
    attribute Boolean userVisible to AttributeTest;
    
    Integer intA = 1;
    
    assign(userVisible = false) to {            
            Integer intB;            
        }
    
    compound Pipeline {
        String name;        
        assign(bindingTime = BindingTime.runtime) to {
            Integer pA;
            Integer pB;            
        }
    }
    
    Pipeline pipeline;
    
}
