project BooleanAlgebraOpTest {

    Boolean t = true;
    Boolean f = false;
    
    // AND
    (t and t) == true;
    (t and f) == false;
    (f and t) == false;
    (f and f) == false;
   
    // OR
    (t or t) == true;
    (t or f) == true;
    (f or t) == true;
    (f or f) == false;
    
    // XOR
    (t xor t) == false;
    (f xor t) == true;
    (t xor f) == true;
    (f xor f) == false;
   
    // NOT
    not(true) == false;
    not(false) == true;
    
}
