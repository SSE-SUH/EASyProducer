project refToCompoundCollectionEqualityTest {

    version v0;

    compound FieldType {
        String name;        
    }
    
    refTo(FieldType) StringType = {name = "String"};
    refTo(FieldType) IntegerType = {name = "Integer"};
    refTo(FieldType) BooleanType = {name = "Boolean"};    
    
    sequenceOf(refTo(FieldType)) sqRefToCompound1 = {StringType, IntegerType, BooleanType};
    sequenceOf(refTo(FieldType)) sqRefToCompound2 = {StringType, IntegerType, IntegerType};
    
    
    sqRefToCompound1 == sqRefToCompound2;

}
