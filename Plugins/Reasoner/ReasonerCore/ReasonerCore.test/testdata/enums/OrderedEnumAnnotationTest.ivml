project EnumAnnotationTest {

    enum AEnum {a1=1, a2=2};
    annotate AEnum ann = AEnum::a1 to EnumAnnotationTest;
    
    enum MyEnum {val1=1, val2=2};
    compound MyCmp {
       Integer i = 1;
    };
    Boolean t = true;
    Integer i = 1;
    Real r = 0.5;
    String s = "aaa";
    MyEnum e = MyEnum::val1;
    MyCmp c = MyCmp{};

    t.ann <> AEnum::a1;
    i.ann <> AEnum::a1;    
    r.ann <> AEnum::a1;    
    s.ann <> AEnum::a1;    
    e.ann <> AEnum::a1;
    c.ann <> AEnum::a1;
    
}
