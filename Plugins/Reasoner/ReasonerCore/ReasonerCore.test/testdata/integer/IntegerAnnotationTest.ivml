project IntegerAnnotationTest {

    annotate Integer ann = 1 to IntegerAnnotationTest;
    
    enum MyEnum {val1, val2};
    compound MyCmp {
       Integer i = 1;
    };
    Boolean t = true;
    Integer i = 1;
    Real r = 0.5;
    String s = "aaa";
    MyEnum e = MyEnum::val1;
    MyCmp c = MyCmp{};

    t.ann <> 1;
    i.ann <> 1;    
    r.ann <> 1;    
    s.ann <> 1;    
    e.ann <> 1;
    c.ann <> 1;
    
}
