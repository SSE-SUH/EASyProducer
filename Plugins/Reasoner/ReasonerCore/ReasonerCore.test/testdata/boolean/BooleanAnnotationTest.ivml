project BooleanAnnotationTest {

    annotate Boolean ann = true to BooleanAnnotationTest;
    
    enum MyEnum {val1, val2};
    Boolean t = true;
    Integer i = 1;
    Real r = 0.5;
    String s = "aaa";
    MyEnum e = MyEnum::val1;

    t.ann <> true;    
    i.ann <> true;    
    r.ann <> true;    
    s.ann <> true;    
    e.ann <> true;    
    
}
