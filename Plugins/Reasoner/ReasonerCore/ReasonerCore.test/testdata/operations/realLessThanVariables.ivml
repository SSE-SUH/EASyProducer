project realGreaterThanVariables {

    Real a;
    Real a1;
    Real b;    
    Real b1;
    
    a=5.2;
    b=2.5;
    a1=-5.2;
    b1=-2.5; 
    
    5.2 < b == false;
    a < 2.5 == false;
    a < b == false;
    -5.2 < b == true;
    a1 < 2.5 == true;
    a1 < b1 == true;
    5.2 < b1 == false;
    a < -2.5 == false;
    a < b1 == false;

}