project integerEqVariables {

    Integer i1 = 1;
    Integer i0 = 0;
    Integer im1 = -1;

    // basic equality (so that we can build on that)
    i1 == i1;
    i0 == i0;
    im1 == im1;
    
    // basic inequality (so that we can build on that)
    i1 <> i0;
    i0 <> i1;
    im1 <> i0;
    i0 <> im1;
    i1 <> im1;
    im1 <> i1;

}