project integerMINVariables {

    Integer a;
    Integer a1;
    Integer b;    
    Integer b1;
    
    a=5;
    b=2;
    a1=-5;
    b1=-2;
    
    min(5, b) == 2;
    min(a, 2) == 2;
    min(a, b) == 2;
    min(-5, b) == -5;
    min(a1, 2) == -5;
    min(a1, b1) == -5;
    min(5, b1) == -2;
    min(a, -2) == -2;
    min(a, b1) == -2;   

}