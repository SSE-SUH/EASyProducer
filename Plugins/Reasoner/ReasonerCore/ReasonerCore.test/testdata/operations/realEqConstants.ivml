project realEqConstants {

    // basic equality (so that we can build on that)
    1.5 == 1.5;
    0.2 == 0.2;
    -1.8 == -1.8;
    
    // basic inequality (so that we can build on that)
    1.5 <> 0.5;
    0.5 <> 1.5;
    -1.5 <> 0.5;
    0.5 <> -1.5;
    1.5 <> -1.5;
    -1.5 <> 1.5;

}