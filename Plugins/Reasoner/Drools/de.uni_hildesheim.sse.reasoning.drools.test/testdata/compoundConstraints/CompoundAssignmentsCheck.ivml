project CompoundAssignmentsCheck {

    compound C {
    
        Integer i1;
        Integer i2;
    }
    
    C c1;
    
    sequenceOf(Integer) seq1;
    seq1 == {1, 3, 5, 6, 7};
    Boolean b1;
    
    b1 == seq1->exists(t | t > 6);
    
    //This should be true
    true and b1;
    
    
    seq1->exists(t | t > 6) implies c1.i1 == 25;
    c1.i2 == 30;
    
    //Error
    c1.i2 == 200;
    
}