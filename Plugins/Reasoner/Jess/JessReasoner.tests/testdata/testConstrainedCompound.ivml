project testConstrainedCompound {

    //Define compound
    compound CMP {
        Integer intA;
        Real realB;
        Integer intC;
    }
    
    //Declaration
    CMP compound1;
    CMP compound2;
    
    //Constraints
    (compound1.intA + compound2.intA) == 15;
    (compound2.realB - compound1.realB) == 5.0;

}